{
	"name": "scg-wp-theme",
	"version": "1.0.0",
	"main": "",
	"scripts": {
		"start": "wp-scripts start --experimental-modules --blocks-manifest",
		"build": "NODE_ENV=production wp-scripts build --experimental-modules --blocks-manifest",
		"build:prod": "npm run lint && npm run build --experimental-modules --blocks-manifest",
		"lint:js": "wp-scripts format && wp-scripts lint-js --fix",
		"lint:style": "wp-scripts lint-style --fix",
		"lint:php": "composer lint:php",
		"tsc": "tsc --noEmit",
		"lint": "npm run lint:js && npm run tsc && npm run lint:style && lint:php",
		"packages-update": "wp-scripts packages-update",
		"test:js": "wp-scripts --no-deprecation test-unit-js",
		"test:js-coverage": "wp-scripts --no-deprecation test-unit-js --collect-coverage",
		"env:start": "wp-env start",
		"env:start-coverage": "wp-env start --xdebug=coverage",
		"test:php": "wp-env run tests-cli --env-cwd=wp-content/themes/scg vendor/bin/phpunit --coverage-html tests/php/coverage"
	},
	"author": "seekthekingdom",
	"license": "ISC",
	"description": "",
	"devDependencies": {
		"@testing-library/jest-dom": "^6.9.1",
		"@testing-library/react": "^16.3.1",
		"@types/wordpress__block-editor": "^14.21.2",
		"@types/wordpress__blocks": "^12.5.18",
		"@wordpress/env": "^10.36.0",
		"@wordpress/icons": "^11.3.0",
		"@wordpress/scripts": "^31.1.0",
		"path": "^0.12.7",
		"typescript": "~5.3.0",
		"webpack-remove-empty-scripts": "^1.1.1"
	},
	"dependencies": {
		"@wordpress/interactivity": "^6.36.0",
		"gsap": "^3.13.0",
		"pdfjs-dist": "^5.4.54"
	}
}
